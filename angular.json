{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "demo-samirromdhani": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss",
              "./node_modules/font-awesome/css/font-awesome.min.css",
              "./node_modules/tether/dist/css/tether.min.css",
              "./node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/styles/vender/css/normalize.css",
              "src/styles/vender/css/owl.css",
              "src/styles/vender/css/animate.css",
              "src/styles/vender/fonts/eleganticons/et-icons.css",
              "src/styles/vender/css/samirromdhani.css",
              "src/styles/vender/css/bootstrap.css"
            ],
            "scripts": [
              "./node_modules/tether/dist/js/tether.min.js",
              "./node_modules/jquery/dist/jquery.min.js",
              "./node_modules/popper.js/dist/umd/popper.min.js",
              "./node_modules/bootstrap/dist/js/bootstrap.min.js",
              "src/styles/vender/js/jquery-1.11.1.min.js",
              "src/styles/vender/js/owl.carousel.min.js",
              "src/styles/vender/js/bootstrap.min.js",
              "src/styles/vender/js/wow.min.js",
              "src/styles/vender/js/typewriter.js",
              "src/styles/vender/js/jquery.onepagenav.js",
              "src/styles/vender/js/main.js",
              "src/styles/vender/js/froala/froala_editor.pkgd.min.js",
              "src/styles/vender/js/froala/fabric.min.js",
              "src/styles/vender/js/froala/tui-code-snippet.min.js",
              "src/styles/vender/js/froala/tui-image-editor.min.js",
              "src/styles/vender/js/froala/image_tui.min.js",
              "src/styles/vender/js/froala/font_awesome.min.js",
              "src/styles/vender/js/froala/fontawesome.min.js",
              "src/styles/vender/js/froala/brands.min.js",
              "src/styles/vender/js/froala/solid.min.js",
              "src/styles/vender/js/froala/v4-shims.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "6mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "demo-samirromdhani:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "demo-samirromdhani:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "demo-samirromdhani:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "demo-samirromdhani:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "demo-samirromdhani:serve:production"
            }
          }
        },
        "deploy": {
          "builder": "angular-cli-ghpages:deploy",
          "options": {}
        }
      }
    }
  },
  "defaultProject": "demo-samirromdhani",
  "cli": {
    "analytics": "d11a4a85-b429-4f19-9632-2275cd4d52e6"
  }
}